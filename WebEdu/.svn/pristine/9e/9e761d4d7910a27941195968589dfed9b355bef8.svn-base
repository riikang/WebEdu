<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
          "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
<session-factory>
	<property name="dialect">
		org.hibernate.dialect.MySQLDialect
	</property>
	<property name="show_sql">true</property>
	<property name="hbm2ddl.auto">update</property>
	<mapping resource="com/WebEdu/DreamFlying/model/Student.hbm.xml" />

	<mapping resource="com/WebEdu/DreamFlying/model/Manager.hbm.xml" />

	<mapping resource="com/WebEdu/DreamFlying/model/Course.hbm.xml" />

	<mapping resource="com/WebEdu/DreamFlying/model/Teacher.hbm.xml" />

</session-factory>
</hibernate-configuration>